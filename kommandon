addsuperuser() {
  if [ "$#" -eq 0 ] ; then
    echo "Usage:\naddsuperuser mattias.bystedt ABGR abgr.admin RÖGR rogr1.admin '0703-597908' [noyubikey]"
  fi
  
  if [ "$#" -gt 0 ] ; then
    len=$#
    i=1
    usercommand="update user "
    admincommand=""
    adminlist=""
    org=""
    for val in $@
    do
      if [[ $i == 1 && $val =~ \. ]] ; then
        usercommand+="$val superUsers.yubiKey true "
        admincommand+="recoveryemail ${val}@skola.malmo.se "
      elif [[ $val =~ ^[[:upper:]]+$ ]] ; then
        if [[ $val =~ GR$ || $val =~ GRS$ || $val =~ GRSU$ || $val =~ EST$  ]] ; then
          org="superUsers.org 171"
        else
          org="superUsers.org 170"
        fi
        usercommand+="superUsers.admin multivalued type custom $val "
      elif [[ $val =~ \.admin$ ]] ; then
        usercommand+="\"${val}\" "
        adminlist+="${val},"
      elif [[ $val =~ ^[0-9].*.[0-9]$ ]] ; then
        usercommand+="superUsers.phone $val "
        recoveryphone=$(echo ${val} | gsed -E 's/^0/+46/' | gsed 's/-//')
        admincommand+="recoveryphone ${recoveryphone}"
      elif [[ $val =~ noyubikey$ ]] ; then
        usercommand=${usercommand// true / false }
      else
        echo "unknown"
      fi
      i=$((i+1))
    done
    usercommand+=$org
    admincommand_result="gam update users ${adminlist} "
    admincommand_result+=$admincommand
    eval "gam ${usercommand}"
    eval "${admincommand_result}"
  fi
}

delsuperuser() {
  gam update user $1 clearschema superUsers
}

superusers() {
  gam print users select group superusers schemas superUsers > ~/gam/superusers-out.csv
  columns="primaryEmail,customSchemas.superUsers.org,customSchemas.superUsers.yubiKey,customSchemas.superUsers.phone,customSchemas.superUsers.admin,"
  columns+=$(csvcut -n ~/gam/superusers-out.csv | ggrep -E "customType|value" | gawk '{printf $NF","}' | gsed -e 's/,$//')
  numof_admins=$(csvcut -c customSchemas.superUsers.admin ~/gam/superusers-out.csv | ggrep '[[:digit:]]' | sort -rn | head -n 1)
  header="SuperAnvändare,org,YubiKey,Mobil,Admins,Enhet,Admin"
  # Add columns for numof_admins
  if [ "$numof_admins" -gt 1 ] ; then 
    for i in {2..$numof_admins} ; do
      header="${header},Enhet${i},Admin${i}"
    done
  fi
  csvcut -c $columns ~/gam/superusers-out.csv | gsed '1d' | gsed "1 i\\${header}" | csvsort -c 2,6 | tee ~/gam/all_superusers.csv | column -t -s,
  if [[ $1 == "update" ]] ; then
    gam user admin update drivefile ${SUPERUSERSFILE} localfile ~/gam/all_superusers.csv
  fi
}
## -- END SUPERUSERS --


### *******************************************************************************************
### -- AD COMMANDS -- 
### *******************************************************************************************

AD() {
  AD_USER_CONF=$(sed -n '7p' ~/gam/.commandconfig.txt)
  echo $AD_USER_CONF | tr -d '\n\r' > ~/gam/.ad_conf.txt
  # AD name 'firstname' 'lastname'
  if [[ $1 == "name" ]] ; then
    ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
      "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
      -y ~/gam/.ad_conf.txt "(&(displayName=${2}* ${3}*))" \
      cn otherMailbox mail displayName department memberOf title physicalDeliveryOfficeName telephoneNumber company \
      adminDescription streetAddress division roomNumber manager mobile departmentNumber > ~/gam/ldap_out.txt
      while read l ; do a=$(echo $l | awk '{print $1}') && b=$(echo $l | awk '{print $2}') && \
      if [[ $a == *"::" ]] ; then
        d=$(echo $b | base64 --decode)
        c=$(echo "${a:0:-1} $d")
		echo $c
	  else
		echo $l
	  fi ; done < ~/gam/ldap_out.txt | tee >(grep memberOf | ggrep -E "17[01]G|SchoolEmployees|EDU_Students" > ~/gam/memberOf.txt) | grep -v memberOf && cat ~/gam/memberOf.txt
  # AD mail 'part-of-email'
  # prints out the name of "google groups"
  elif [[ $1 =~ ^(mail|email)$ ]] ; then
    rm ~/gam/memberOf.txt
    printf "(|" > ~/gam/memberOf.txt
    ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
        "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
        -y ~/gam/.ad_conf.txt "(&(mail=${2}*))" \
        cn otherMailbox mail displayName givenName sn department memberOf title physicalDeliveryOfficeName telephoneNumber company \
        adminDescription streetAddress division roomNumber manager mobile departmentNumber \
        pwdLastSet whenCreated accountExpires pager extensionName houseIdentifier lastLogonTimestamp > ~/gam/ldap_out.txt
    while read l
    do
      a=$(echo $l | awk '{print $1}')
      b=$(echo $l | awk '{print $2}')
      if [[ $a == *"::" ]] ; then
        c=$(echo $b | base64 --decode)
        l=$(echo "${a:0:-1} $c")
        a=$(echo $l | awk '{print $1}')
        b=$(echo $l | awk '{print $2}')
      fi
      if [[ $a == "memberOf:" ]] ; then
        g=$(echo $b | awk -F, '{print $1}' | awk -F"=" '{print $2}')
        printf "(CN=${g})" | ggrep -E "17[01]G|SchoolEmployees|EDU_Students" >> ~/gam/memberOf.txt
      elif [[ $a == "mail:" ]] ; then
        echo $l
        echo "groups:"
        printf ")" >> ~/gam/memberOf.txt
        groups=$(cat ~/gam/memberOf.txt)
        ADgroup "$groups"
        # If multiple hits - remove previous users groups from list
        #rm ~/gam/memberOf.txt
      else
        if [[ $a == "pwdLastSet:" ]] ; then
          d=$(ldaptime $b)
          echo $a $d
        elif [[ $a == "lastLogonTimestamp:" ]] ; then
          d=$(ldaptime $b)
          echo $a $d
        elif [[ $a == "accountExpires:" ]] ; then
          d=$(ldaptime $b)
          echo $a $d
        else
          echo $l
        fi
      fi
    done < ~/gam/ldap_out.txt #\
  elif [[ $1 =~ ^(cn|CN)$ ]] ; then
    ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
      "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
      -y ~/gam/.ad_conf.txt "(&(CN=${2}))" \
      cn otherMailbox mail displayName department memberOf title physicalDeliveryOfficeName telephoneNumber company \
      adminDescription streetAddress division roomNumber manager mobile departmentNumber \
       > ~/gam/ldap_out.txt  && while read l ; do a=$(echo $l | awk '{print $1}') && b=$(echo $l | awk '{print $2}') && \
      if [[ $a == *"::" ]] ; then d=$(echo $b | base64 --decode) &&  c=$(echo "${a:0:-1} $d") && echo $c ; else echo $l ; fi ; done \
      < ~/gam/ldap_out.txt | tee >(grep memberOf > ~/gam/memberOf.txt) | grep -v memberOf && cat ~/gam/memberOf.txt
  elif [[ $1 =~ ^(eppn|EPPN)$ ]] ; then
    ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
      "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
      -y ~/gam/.ad_conf.txt "(&(extensionName=${2}))" \
      cn otherMailbox mail displayName department title physicalDeliveryOfficeName telephoneNumber company \
      adminDescription streetAddress division roomNumber manager mobile departmentNumber \
       > ~/gam/ldap_out.txt  && while read l ; do a=$(echo $l | awk '{print $1}') && b=$(echo $l | awk '{print $2}') && \
      if [[ $a == *"::" ]] ; then d=$(echo $b | base64 --decode) &&  c=$(echo "${a:0:-1} $d") && echo $c ; else echo $l ; fi ; done \
      < ~/gam/ldap_out.txt | tee >(grep memberOf > ~/gam/memberOf.txt) | grep -v memberOf && cat ~/gam/memberOf.txt
  elif [[ $1 =~ ^(othermail)$ ]] ; then
    ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
      "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
      -y ~/gam/.ad_conf.txt $2 \
      otherMailbox > ~/gam/ldap_othermail.txt
    while read l ; do
      a=$(echo $l | awk '{print $1}')
      b=$(echo $l | awk '{print $2}')
      if [[ $a == "otherMailbox:" ]] ; then
        echo $b
      fi
    done < ~/gam/ldap_othermail.txt
  else
    echo "unknown command"
  fi
  AD_USER_CONF="X"
  echo $AD_USER_CONF | tr -d '\n\r' > ~/gam/.ad_conf.txt
}

ADgroup() {
  ldapsearch -LLL -o ldif-wrap=no -H $AD_URL -b \
      "OU=Malmo,DC=intra,DC=malmo,DC=se" -D "CN=${AD_USER},OU=Employees,OU=ADM,OU=Malmo,DC=intra,DC=malmo,DC=se" \
      -y ~/gam/.ad_conf.txt $1 \
      displayName adminDisplayName > ~/gam/ldap_group.txt
  while read l
  do
    a=$(echo $l | awk '{print $1}')
    b=$(echo $l | awk '{print $2}')
    if [[ $a == *"::" ]] ; then
      a=$(echo $l | awk '{print $1}')
      b=$(echo $b | base64 --decode)
      #b=$(echo "${a:0:-1} $c")
    fi
    if [[ $a =~ "displayName" ]] ; then
      echo $b
    fi
  done < ~/gam/ldap_group.txt
}

ldaptime() {
  u=$(echo "$(($1 / 10000000))") && t=$(echo "$(($u - 11644473600))") && gdate -d @$t +'%Y-%m-%d %H:%M:%S'
}
## --- END AD COMMANDS ---


